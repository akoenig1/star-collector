{
	"name": "star-collector",
	"version": "0.0.1",
	"private": true,
	"main": "index.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"generate-migration": "drizzle-kit generate --dialect=postgresql --out=src/lib/db/migrations --schema=src/lib/db/schema.ts",
		"migrate": "tsx src/lib/db/migrate.ts",
		"upgrade-migrations": "drizzle-kit up --dialect=postgresql --out=src/lib/db/migrations",
		"drizzle-studio": "drizzle-kit studio",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/pg": "^8.11.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.17",
		"dotenv": "^16.4.4",
		"drizzle-kit": "^0.24.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"lucia": "^3.1.1",
		"postcss": "^8.4.35",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-superforms": "^2.12.4",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"tsx": "^4.7.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"@vercel/postgres": "^0.9.0",
		"drizzle-orm": "^0.33.0",
		"oslo": "^1.2.0",
		"pg": "^8.11.3",
		"svelecte": "^4.2.5"
	}
}
